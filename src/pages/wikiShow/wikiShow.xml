<!--文化百科对应视频详情-->
<video objectFit="cover" class="w-video" src="{{ info.videoSrc }}"></video>
<view class="v-w">
  <text class="v-title f30 c3">{{ info.title }}</text><text>{{ info.content }}</text>
</view>

<view class="w-list">
  <view class="w-l-w f30 c3 pr" wx:for="{{ info.list }}" wx:for-item="list">
    <image src="{{ list.src }}"></image>
    <view class="pa tac br w-p iconfont icon-bofang1"></view>
    <text>{{ list.t }}</text>
  </view>
</view>