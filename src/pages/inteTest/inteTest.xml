<!--智能测试页面-->
<view wx:for="{{ lists }}" class="wiki">
  <view class="w-t f30 c3">{{ item.title }}</view>
  <view class="w-list">
    <view class="w-l-w f30 c3 pr" style="width: {{ item.width }}rpx; height: {{ item.height }}rpx;" wx:for="{{ item.list }}" wx:for-item="list">
      <image catchtap="goNext" data-title="{{ list.name }}" data-type="{{ item.id }}" data-id="{{ list.id }}" mode="aspectFill" src="{{ list.image }}"></image>
      <!--<view class="pa tac br w-p iconfont icon-bofang1"></view>-->
      <!--<text>{{ list.t }}</text>-->
    </view>
  </view>
</view>