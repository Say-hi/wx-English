<!--词典页面-->
<view class="d-top">
  <view class="d-t-s">
    <input data-type="translate" class="f30 c3" placeholder="输入要查询的内容"/>
    <view catchtap="getTran" class="f30 c3 d-t-s-b">翻译</view>
  </view>
  <view class="d-t-b f30 c3">
    <view class="d-voice">
      英[{{ info.en }}]<text catchtap="playVoice" class="iconfont icon-shengyin"></text>
    </view>
    <view class="d-voice">
      美[{{ info.us }}]<text catchtap="playVoice" class="iconfont icon-shengyin"></text>
    </view>
  </view>
</view>

<view class="d-bottom">
  <text class="f30 c3">权威例句</text>
  <view class="d-b-c">
    <view class="d-b-c-i" wx:for="{{ lists }}">
      {{ index + 1 }}. <text>{{ item.en }}</text>
      <view>{{ item.cn }}</view>
    </view>
  </view>
</view>